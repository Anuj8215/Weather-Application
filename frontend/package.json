{
  "name": "frontend",
  "version": "1.0.0",
  "description": "React Native Weather App with TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "npx react-native clean-project-auto",
    "clean:cache": "react-native start --reset-cache",
    "postinstall": "npx pod-install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-native": "^0.75.4",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "react-native-screens": "^3.34.0",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-gesture-handler": "^2.19.0",
    "@reduxjs/toolkit": "^2.2.7",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "axios": "^1.7.7",
    "react-native-vector-icons": "^10.2.0",
    "react-native-linear-gradient": "^2.8.3",
    "@react-native-community/geolocation": "^3.3.0",
    "react-native-permissions": "^4.1.5",
    "react-hook-form": "^7.53.0",
    "@hookform/resolvers": "^3.9.0",
    "zod": "^3.23.8",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@react-native/babel-preset": "^0.75.4",
    "@react-native/metro-config": "^0.75.4",
    "@react-native/typescript-config": "^0.75.4",
    "typescript": "^5.6.3",
    "@types/react": "^18.3.11",
    "@types/react-native": "^0.73.0",
    "@types/lodash": "^4.17.10",
    "@testing-library/react-native": "^12.7.2",
    "@testing-library/jest-native": "^5.4.3",
    "jest": "^29.7.0",
    "react-test-renderer": "^18.3.1",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-native": "^4.1.0",
    "prettier": "^3.3.3",
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "babel-jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react-native",
    "typescript",
    "weather-app",
    "mobile"
  ],
  "author": "Your Name",
  "license": "MIT",
  "private": true
}